Cell[BoxData[
 RowBox[{
  RowBox[{"distancebayes", "[", 
   RowBox[{"p_", ",", "m_"}], "]"}], ":=", 
  RowBox[{"Total", "[", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{"Function", "[", 
      RowBox[{"c", ",", 
       RowBox[{"PDF", "[", 
        RowBox[{
         RowBox[{"MultinormalDistribution", "[", 
          RowBox[{"c", ",", 
           RowBox[{
            RowBox[{"IdentityMatrix", "[", "2", "]"}], "*", "stdev"}]}], 
          "]"}], ",", "p"}], "]"}]}], "]"}], ",", "m"}], "]"}], 
   "]"}]}]], "Input",
 CellLabel->
  "In[181]:=",ExpressionUUID->"5f7a429a-7438-4666-a18f-e0cd6b4b20e7"],

Cell[BoxData[
 RowBox[{
  RowBox[{"classbayes", "[", "p_", "]"}], ":=", 
